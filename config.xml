<?xml version="1.0" encoding="UTF-8"?>

<widget
	xmlns       = "http://www.w3.org/ns/widgets"
	xmlns:gap   = "http://phonegap.com/ns/1.0"
	id          = "nl.avans.aii.cmd.SpookyApp"
	version     = "1.0.0"
	versionCode = "100"
    targetSdkVersion = "14"
>

	<name>Spooky App</name>
    
	<description>The spookiest app ever made.</description>

	<author href="http://www.avans.nl" email="imbaaije@avans.nl">
		Ion Baaijens
	</author>
    
	<preference name="permissions" value="none"/>

	<icon src="icon.png" /> <!-- 128x128 -->
	<splash src="splash.png" /> <!-- 320x480 -->

    <plugin name="cordova-plugin-device-orientation" spec=" 1.0.5" />
    <plugin name="cordova-plugin-vibration" spec="2.1.3" />
    <plugin name="cordova-plugin-network-information" spec="1.3.1" />
    
    
</widget>